{
  "name": "react-redux-passport-uikit-express-boiler",
  "version": "0.0.1",
  "description": "A React+Redux boilerplate using Express as backend, UIKit for frontend, MongoDB for storage & Passport for auth.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "webpack -p",
    "deploy": "npm run build && git commit -a -m deployment && git push heroku master",
    "dev": "webpack-dev-server --open",
    "server": "cross-env NODE_ENV=production babel-node src/server",
    "server-dev": "nodemon src/server --exec babel-node src/server",
    "start-dev": "concurrently \"npm run server-dev\" \"npm run dev\"",
    "server-spec": "babel-node src/server/server.spec",
    "start": "npm run server"
  },
  "author": "Scharkee",
  "repository": {
    "type": "git",
    "url": "https://repo.hepha.co.uk/root/react-redux-passport-uikit-express-boiler"
  },
  "license": "ISC",
  "dependencies": {
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "mongoose": "^5.6.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.1.0",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "redux": "^4.0.1",
    "redux-saga": "^1.0.3",
    "webpack": "^4.34.0"
  }
}
